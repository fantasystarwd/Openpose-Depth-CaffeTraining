net: "models/Openpose-Depth/pose_train.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.000080
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "step"
gamma: 0.333
stepsize: 136106
# Display every 100 iterations
display: 500
# The maximum number of iterations
max_iter: 300000
# snapshot intermediate results
snapshot: 500
snapshot_prefix: "models/Openpose-Depth/Result/pose"
# solver mode: CPU or GPU
solver_mode: GPU
